{
  "version": 2,
  "builds": [
    {
      "src": "build/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
