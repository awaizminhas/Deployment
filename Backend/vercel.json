{
  "version": 2,
  "builds": [
    {
      "src": "build/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server.js"
    },
     {
      "src": "/login-driver",
      "dest": "/build"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
